program RuneCrafting;


Function MSI_SetupRunecrafting:Boolean;
Var TiaraFound:Boolean;
    tiara     :Integer;
Begin

  tiara := MSI_Players[CurrentPlayer].Scripts[CurrentScript].WhichTiara
  TiaraFound := MSI_FindTool(tiara,TAB_INV,x,y);
  If TiaraFound Then
    MSI_ClickItem(tiara,['ear']);

 If Not TiaraFound Then
   tiaraFound :=  MSI_FindTool(tiara, TAB_EQUIP, x, y);

 If TiaraFound Then
   Result := MSI_SetupCooking;

End;




