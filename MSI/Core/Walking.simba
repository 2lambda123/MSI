    LOC_DB_BANK: pArr := [TPointArray([Point(3231, 3218), Point(3235, 3223), Point(3227, 3233),
             Point(3221, 3245), Point(3217, 3258), Point(3217, 3272),
             Point(3206, 3278), Point(3192, 3282), Point(3178, 3285),
             Point(3164, 3287), Point(3151, 3294), Point(3139, 3294),
             Point(3126, 3297), Point(3113, 3294), Point(3109, 3280),
             Point(3105, 3267), Point(3105, 3254), Point(3099, 3248),
             Point(3094, 3242)])];
    LOC_FE_BANK: pArr := [TPointArray([Point(3229, 3217), Point(3234, 3223), Point(3226, 3236),
             Point(3219, 3249), Point(3216, 3263), Point(3211, 3276),
             Point(3197, 3280), Point(3184, 3285), Point(3171, 3285),
