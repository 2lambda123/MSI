             Point(3096, 3453), Point(3099, 3465), Point(3097, 3478),
             Point(3088, 3485), Point(3089, 3492), Point(3093, 3491)])];
    LOC_SV_BANK:
                 begin
                   pArr := [];
                   obs := [];
                 end;
