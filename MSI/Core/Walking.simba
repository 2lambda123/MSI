             Point(3295, 3237), Point(3282, 3228), Point(3282, 3213),
             Point(3284, 3198), Point(3283, 3183), Point(3278, 3170),
             Point(3270, 3165)])];
    LOC_KJ_STILES:
                  begin
                    pArr := [];
                    obs := [];
                  end;
    LOC_CB_BANK:
                 begin
                   pArr := [];
                   obs := [];
                 end;
