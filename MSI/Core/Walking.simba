             Point(2962, 3379), Point(2952, 3380), Point(2946, 3368)])];
    LOC_PS_DBOX: pArr := [TPointArray([Point(3228, 3218), Point(3237, 3219), Point(3230, 3230),
             Point(3223, 3242), Point(3217, 3253), Point(3216, 3267),
             Point(3209, 3277), Point(3196, 3280), Point(3185, 3283),
             Point(3171, 3286), Point(3157, 3291), Point(3143, 3293),
             Point(3130, 3296), Point(3117, 3294), Point(3105, 3293),
             Point(3092, 3288), Point(3077, 3283), Point(3071, 3273),
             Point(3057, 3265), Point(3044, 3258), Point(3041, 3244),
             Point(3047, 3233)])];
    LOC_EV_BANK: pArr := [TPointArray([Point(3230, 3217), Point(3242, 3224), Point(3257, 3226),
             Point(3260, 3238), Point(3254, 3250), Point(3251, 3263),
             Point(3242, 3275), Point(3239, 3290), Point(3241, 3306),
